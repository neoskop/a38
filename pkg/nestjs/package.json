{
    "name": "@a38/nestjs",
    "version": "0.0.0",
    "exports": {
        ".": {
            "bun": "./src/index.ts",
            "require": "./libs/index.js",
            "import": "./lib/index.mjs",
            "types": "./src/index.ts"
        }
    },
    "main": "./lib/index.js",
    "module": "./lib/index.mjs",
    "types": "./src/index.ts",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --out-dir lib",
        "lint": "biome lint .",
        "lint:changed": "biome check . --changed --no-errors-on-unmatched",
        "lint:staged": "biome check . --staged --no-errors-on-unmatched --write",
        "test": "bun test",
        "test:coverage": "bun test --coverage",
        "test:watch": "bun test --watch"
    },
    "devDependencies": {
        "@a38/biome-config": "0.0.1",
        "@a38/core": "0.0.2",
        "@a38/tsconfig": "0.0.1",
        "@a38/typedoc": "0.0.1",
        "@nestjs/common": "10.4.1",
        "@nestjs/core": "10.4.1",
        "@nestjs/graphql": "12.2.0",
        "@nestjs/platform-express": "10.4.1",
        "@nestjs/testing": "10.4.1",
        "@types/node": "20.16.2",
        "@types/supertest": "6.0.2",
        "supertest": "7.0.0",
        "typescript": "5.5.4"
    },
    "peerDependencies": {
        "@a38/core": "*",
        "@nestjs/common": "*",
        "@nestjs/core": "*",
        "@nestjs/graphql": "*"
    },
    "peerDependenciesMeta": {
        "@nestjs/graphql": {
            "optional": true
        }
    }
}
